<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Pos?>

<VBox spacing="20" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ReclamationListController" style="-fx-background-color: #F1F2F6; -fx-padding: 20;">

    <!-- Title -->
    <Label text="Reclamations List" style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: #2C3E50; -fx-effect: dropshadow(gaussian, #3e5bb2, 10, 0.5, 2, 2); -fx-padding: 10;"/>

    <!-- ListView to display reclamations with a rounded border and soft background -->
    <ListView fx:id="reclamationListView" prefHeight="300" style="-fx-background-color: #FFFFFF; -fx-border-radius: 10px; -fx-border-color: #3498DB; -fx-border-width: 2px; -fx-font-size: 16px; -fx-padding: 10; -fx-effect: dropshadow(gaussian, rgba(0, 0, 0, 0.1), 10, 0.5, 2, 2);"/>

    <!-- Back Button with attractive colors -->
    <Button text="Back" style="-fx-background-color: #E74C3C; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 16px; -fx-padding: 10; -fx-border-radius: 8px; -fx-effect: dropshadow(gaussian, #E74C3C, 10, 0.5, 2, 2);" onAction="#goBack"/>
</VBox>