<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ScoreControl">
    <children>
        <!-- Search Field -->
        <TextField fx:id="rechercheFild" layoutX="107.2" layoutY="19.0" prefHeight="26.0" prefWidth="144.0" AnchorPane.leftAnchor="107.0" AnchorPane.rightAnchor="549.0" AnchorPane.topAnchor="19.0" />
        <!-- ListView -->
        <ListView fx:id="listview" prefHeight="241.0" prefWidth="755.0" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="20" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="60" />
        <!-- Delete Button -->
        <Button fx:id="deleteButton" layoutX="406.0" layoutY="19.0" onAction="#deleteScore" prefHeight="26.0" prefWidth="95.0" text="Supprimer" />
        <Label layoutX="20.0" layoutY="24.0" text="Recherche" />
        <Label fx:id="messagelist" layoutX="20.0" layoutY="19.0" />
        <Button fx:id="backButton" layoutX="542.0" layoutY="19.0" onAction="#goToGestion" prefHeight="26.0" prefWidth="95.0" text="Retour" />
        <ComboBox fx:id="quizFilter" layoutX="267.0" layoutY="19.0" onAction="#filterByQuiz" prefHeight="26.0" prefWidth="88.0" />
    </children>
</AnchorPane>
