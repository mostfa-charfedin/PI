<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.Pos?>

<VBox spacing="20" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReclamationListController">
    <!-- Title -->
    <Label text="Reclamations List" style="-fx-font-size: 24px; -fx-font-weight: bold; -fx-text-fill: #1850F8; -fx-effect: dropshadow(gaussian, #3e5bb2, 10, 0.5, 2, 2); -fx-padding: 10;"/>

    <!-- ListView to display reclamations -->
    <ListView fx:id="reclamationListView" prefHeight="300" style="-fx-font-size: 16px;"/>

    <!-- Back Button -->
    <Button text="Back" style="-fx-background-color: #f44336; -fx-text-fill: white; -fx-font-weight: bold; -fx-font-size: 16px; -fx-padding: 10;" onAction="#goBack"/>
</VBox>
